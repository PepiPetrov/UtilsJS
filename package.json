{
  "version": "0.1.2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "js-class-utilities",
  "author": "PepiPetrov",
  "module": "dist/utilsjs.esm.js",
  "size-limit": [
    {
      "path": "dist/js-class-utilities.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/js-class-utilities.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/esbuild-why": "^8.2.4",
    "@size-limit/preset-small-lib": "^8.2.4",
    "@types/node": "^18.15.3",
    "husky": "^8.0.3",
    "size-limit": "^8.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "wildcard-match": "^5.1.2"
  },
  "repository": "https://github.com/PepiPetrov/UtilsJS"
}
